{
  "name": "proposal-planner",
  "productName": "Proposal Planner",
  "description": "Application to manage clients and proposals",
  "version": "1.0.0",
  "author": "Kyle Steike",
  "private": true,
  "proxy": "http://127.0.0.1:4000",
  "main": "public/electron.js",
  "homepage": "./",
  "devDependencies": {
    "electron": "^25.0.1",
    "electron-builder": "^23.6.0"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@material-table/core": "^6.1.15",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.3",
    "@react-pdf/renderer": "^3.1.11",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.34",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "axios": "^1.4.0",
    "bootstrap": "^5.3.0",
    "concurrently": "^8.1.0",
    "cross-env": "^7.0.3",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.4",
    "react-dom": "^18.2.0",
    "react-icons": "^4.9.0",
    "react-number-format": "^5.2.2",
    "react-router-dom": "^6.11.2",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "wait-on": "^7.0.1",
    "zustand": "^4.3.8"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "electron .",
    "proposal-server": "cd ../backend && node server.js",
    "electron:serve": "concurrently -k \"cross-env BROWSER=none npm start \" \"npm run electron:start\" ",
    "electron:build": "npm run build && electron-builder -c.extraMetadata.main=build/main.js",
    "electron:start": "wait-on http://127.0.0.1:3000 && electron .",
    "package": "build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.KyleSteike.ProposalPlanner",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ]
  }
}
