{
  "name": "proposal-planner",
  "productName": "Proposal Planner",
  "description": "Application to manage clients and proposals",
  "version": "1.0.0",
  "author": "Kyle Steike",
  "private": true,
  "proxy": "http://127.0.0.1:4000",
  "main": "backend/electron.js",
  "homepage": "./",
  "devDependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@material-table/core": "^6.1.15",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.3",
    "@react-pdf/renderer": "^3.1.11",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/jest": "^27.5.2",
    "@types/lodash": "^4.14.195",
    "@types/node": "^16.18.34",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "axios": "^1.4.0",
    "bootstrap": "^5.3.0",
    "cross-env": "^7.0.3",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.4",
    "react-dom": "^18.2.0",
    "react-icons": "^4.9.0",
    "react-number-format": "^5.2.2",
    "react-router-dom": "^6.11.2",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "zustand": "^4.3.8"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.8",
    "@fullcalendar/daygrid": "^6.1.8",
    "@fullcalendar/interaction": "^6.1.8",
    "@fullcalendar/list": "^6.1.8",
    "@fullcalendar/react": "^6.1.8",
    "@fullcalendar/timegrid": "^6.1.8",
    "@types/axios": "^0.14.0",
    "axios": "^1.4.0",
    "immutability-helper": "^3.1.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "move-client": "mv build ../server/dist/client || move build ../server/dist/client",
    "react-build": "react-scripts build && yarn run move-client",
    "start": "cross-env BROWSER=none yarn run react-start",
    "start:browser": "yarn run react-start"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.KyleSteike.ProposalPlanner",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "package.json",
      "backend/**/*"
    ],
    "extraFiles": [
      "backend/**/*"
    ]
  }
}
